<div class="row">
  <div class="col-sm-12">
    <h4>Add Product IMEI</h4>
  </div>
  <form class="row" action="/sale/add_imei/<%= saleProd[0].id %>" method="post">
  <div class="col-sm-12">
      <% for( var prod of products){
          if(saleProd.product == prod.id){
        %>
          <label class="pull-left">Title: <%= prod.title %></label>
        <% } } %>
  </div>
        <div class="col-sm-12">
          <table class="table table-striped table-hover table-sm data-tables">
            <thead class="thead-dark">
                  <tr>
                    <th></th>
                    <th>IMEI</th>
                    <th>Status</th>
                  </tr>
                </thead>
                <tbody>
                  <% for(var c of col){ %>
                    <tr>
                      <td>
                        <div class="form-group checkbox-warning-filled">
                            <input type="checkbox" name="imei[]" value="<%= c.id %>" id="ch_<%= c.id %>" class="filled-in">
                            <input type="hidden" name="imei_id[]" value="<%= c.id %>">
                            <label for="<%= c.id %>"></label>
                        </div>
                      </td>
                      <td><%= c.imei %></td>
                      <td><%= c.status %></td>
                    </tr>
                  <% } %>
                </tbody>
              </table>

        </div>
        <input type="hidden" name="id" value="<%= saleProd[0].sale %>">

      <div class="clearfix"></div>
      <div class="form-group col-sm-12 padding-15">
        <button type="submit" class="btn btn-success">Submit</button>
      </div>
    </form>
</div>
